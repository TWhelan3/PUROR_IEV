<?xml version="1.0" encoding="UTF-8"?>
<gadgetronStreamConfiguration xmlns="http://gadgetron.sf.net/gadgetron" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://gadgetron.sf.net/gadgetron gadgetron.xsd">

	<gadget>
		<name>LoadIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>LoadIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
			<value>data-me.ismrmrd</value>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>complex</value>
		</property>
	</gadget>
	
	<gadget>
		<name>GetMasks</name>
		<dll>gadgetron_PUROR</dll>
		<classname>GetMaskGadget</classname>
		<property>
			<name>maskflag</name>
			<value>0</value>
		</property>
	</gadget>
	<gadget>
		<name>Unwrap</name>
		<dll>gadgetron_PUROR</dll>
		<classname>UnwrapGadget</classname>
		  <property><name>threads</name><value>1</value></property>
	</gadget>
		<gadget>
		<name>HighPassFilter</name>
		<dll>gadgetron_PUROR</dll>
		<classname>HPFGadget</classname>
		<property>
			<name>filter_type</name>
			<value>1</value>
		</property>
		<property>
			<name>sigma</name>
			<value>.01</value>
		</property>
	</gadget>
	<gadget>
		<name>Unwrap3D</name>
		<dll>gadgetron_PUROR</dll>
		<classname>Unwrap3DGadget</classname>
	</gadget>
	<gadget>
		<name>Accumulator</name>
		<dll>gadgetron_PUROR</dll>
		<classname>AccGadget</classname>
		<property>
			<name>iev</name>
			<value>0</value>
		</property>
		<property>
			<name>norm</name>
			<value>0</value>
		</property>
	</gadget>
	<gadget>
		<name>Contrast</name>
		<dll>gadgetron_PUROR</dll>
		<classname>ContrastGadget</classname>
		<property>
			<name>pass_on</name>
			<value>0</value>
		</property>
	</gadget>
	<gadget>
	    <name>AutoScaleEntireImage</name>
	      <dll>cfmm_gadgets</dll>
	      <classname>AutoScaleEntireImage</classname>
 	 </gadget>
	<gadget>
		<name>SaveIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>SaveIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
			<value>smallout_noscale.ismrmrd</value>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>complex</value>
		</property>
		<property>
			<name>pass_on_undesired_data</name>
			<value>true</value>
		</property>
		<property>
			<name>workingDirectory</name>
			<value>~/local/sandbox</value>
		</property>
	</gadget>
</gadgetronStreamConfiguration>
