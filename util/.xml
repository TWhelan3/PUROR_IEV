<?xml version="1.0" encoding="UTF-8"?>
<gadgetronStreamConfiguration xmlns="http://gadgetron.sf.net/gadgetron" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://gadgetron.sf.net/gadgetron gadgetron.xsd">
	<gadget>
		<name>LoadIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>LoadIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
			<value>../large.ismrmrd</value>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>complex</value>
		</property>
	</gadget>
	<gadget>
		<name>GetMasks</name>
		<dll>gadgetron_PUROR</dll>
		<classname>GetMaskGadget</classname>
	</gadget>
	<gadget>
		<name>Unwrap</name>
		<dll>gadgetron_PUROR</dll>
		<classname>UnwrapGadget</classname>
		 <property><name>threads</name><value>6</value></property>
		<!--<property><name>savephase</name><value>0</value></property>-->
	</gadget>
	<!--<gadget>
		<name>Unwrap3D</name>
		<dll>gadgetron_PUROR</dll>
		<classname>Unwrap3DGadget</classname>
		<property><name>savephase</name><value>0</value></property>
	</gadget>-->
	<gadget>
		<name>HighPassFilter</name>
		<dll>gadgetron_PUROR</dll>
		<classname>HPFGadget</classname>
		<property>
			<name>sigma</name>
			<value>.015</value>
		</property>
		<!--<property>
			<name>output</name>
			<value>output@IEVChannelSum</value>
		</property>-->
	</gadget>
	<gadget>
		<name>Accumulator</name>
		<dll>gadgetron_PUROR</dll>
		<classname>IEVChannelSumGadget</classname>
		<property>
			<name>iev</name>
			<value>1</value>
		</property>
		<property>
			<name>output</name>
			<value>1</value>
		</property>
	</gadget>
	<gadget>
		<name>Contrast</name>
		<dll>gadgetron_PUROR</dll>
		<classname>ContrastGadget</classname>
		<property>
			<name>pass_on</name>
			<value>0</value>
		</property>
	</gadget>
	<gadget>
		<name>AutoScale</name>
		<dll>gadgetron_mricore</dll>
		<classname>AutoScaleGadget</classname>
	</gadget>
	<gadget>
		<name>DICOMSave</name>
		<dll>cfmm_gadgets</dll>
		<classname>DicomSave</classname>
		<property>
		<name>dicom_dir</name>
		</property>
	</gadget>
</gadgetronStreamConfiguration>