	<gadget>
		<name>AutoScaleEntireImage</name>
		<dll>cfmm_gadgets</dll>
		<classname>AutoScaleEntireImage</classname>
		<property>
		<name>numEchos</name>
		</property>
		<property>
		<name>is_signed</name>
		<value>true</value>
		</property>
	</gadget>
	<gadget>
		<name>DICOMStream</name>
		<dll>cfmm_gadgets</dll>
		<classname>DicomStream</classname>
	</gadget>
	<gadget>
		<name>AddDicomSlopeIntercept</name>
		<dll>gadgetron_PUROR</dll>
