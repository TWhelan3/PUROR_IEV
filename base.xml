	<gadget>
		<name>LoadIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>LoadIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>complex</value>
		</property>
	</gadget>
	<gadget>
		<name>DecrementIndex</name>
		<dll>gadgetron_PUROR</dll>
		<classname>DecImageIndex</classname>
	</gadget>
	<gadget>
		<name>GetMasks</name>
		<dll>gadgetron_PUROR</dll>
		<classname>GetMaskGadget</classname>
	</gadget>
	<gadget>
		<name>Unwrap</name>
		<dll>gadgetron_PUROR</dll>
		<classname>UnwrapGadget</classname>
		 <property><name>threads</name><value>6</value></property>
	</gadget>
	<gadget>
		<name>SaveIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>SaveIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>phase</value>
		</property>
		<property>
			<name>pass_on_undesired_data</name>
			<value>true</value>
		</property>
	</gadget>
	<gadget>
		<name>HighPassFilter</name>
		<dll>gadgetron_PUROR</dll>
		<classname>HPFGadget</classname>
		<property>
			<name>sigma</name>
			<value>.005</value>
		</property>
	</gadget>
	<gadget>
		<name>Accumulator</name>
		<dll>gadgetron_PUROR</dll>
		<classname>IEVChannelSumGadget</classname>
	</gadget>
