<?xml version="1.0" encoding="UTF-8"?>
<gadgetronStreamConfiguration xmlns="http://gadgetron.sf.net/gadgetron" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://gadgetron.sf.net/gadgetron gadgetron.xsd">

	<gadget>
		<name>LoadIsmrmrdDatasetImages</name>
		<dll>cfmm_gadgets</dll>
		<classname>LoadIsmrmrdDatasetImages</classname>
		<property>
			<name>filename</name>
			<value>data-me.ismrmrd</value>
		</property>
		<property>
			<name>groupname</name>
			<value>images</value>
		</property>
		<property>
			<name>varname</name>
			<value>complex</value>
		</property>
	</gadget>
	
	<gadget>
		<name>GetMasks</name>
		<dll>gadgetron_PUROR</dll>
		<classname>GetMaskGadget</classname>
		<property>
			<name>maskflag</name>
			<value>0</value>
		</property>
	</gadget>
	<gadget>
		<name>Unwrap</name>
		<dll>gadgetron_PUROR</dll>
		<classname>UnwrapGadget</classname>
		  <property><name>threads</name><value>1</value></property>
	</gadget>
	<gadget>
		<name>HighPassFilter</name>
		<dll>gadgetron_PUROR</dll>
		<classname>HPFGadget</classname>
		<property>
			<name>filter_type</name>
			<value>1</value>
		</property>
	</gadget>
	<gadget>
		<name>Unwrap3D</name>
		<dll>gadgetron_PUROR</dll>
		<classname>Unwrap3DGadget</classname>
	</gadget>

	<gadget>
		<name>Accumulator</name>
		<dll>gadgetron_PUROR</dll>
		<classname>AccGadget</classname>
	</gadget>
	<!--<gadget>
      <name>AutoScaleEntireImage</name>
      <dll>cfmm_gadgets</dll>
      <classname>AutoScaleEntireImage</classname>
 	 </gadget>-->

     <gadget>
      <name>AutoScale</name>
      <dll>gadgetron_mricore</dll>
      <classname>AutoScaleGadget</classname>
      <property>
         <name>max_value</name>
         <value>4095</value>
      </property>
  	 </gadget>

	<gadget>
		<name>DicomSend</name>
		<dll>cfmm_gadgets</dll>
		<classname>DicomSend</classname>
		<property>
			<name>peerHostName</name>
			<value>192.168.122.10 </value>
		</property>
		<property>
		<name>peerPort</name>
		
		<value>11112</value>
		</property>
		<property>
		<name>peerAETitle</name>
		
		<value>CFMM-public</value>
		</property>
		<property>
		<name>callingAETitle</name>
		
		<value>gadgetron_streamer</value>
		</property>
	</gadget>

</gadgetronStreamConfiguration>
